<%- include("../partials/header") %>
<h1>This is <%= campground.name %></h1>
<form action="/campgrounds/<%= campground._id %>?_method=PUT" method="POST">
    <div class="form-group">
    <input type="text" name="camp[name]" value="<%= campground.name %>" >
    </div>
    <div class="form-group">
    <input type="text" name="camp[image]" value="<%= campground.image %>" >
    </div>
    <div class="form-group">
        <input type="text" name="camp[description]" value="<%= campground.description %>" >
        </div>
        <div class="form-group">
            <input type="text" value="<%= campground.location %>" name="camp[location]">
            </div>
            <div class="form-group">
                <input type="text" value="<%= campground.sublocation %>" name="camp[sublocation]">
                </div>
                <div class="form-group">
                    <input type="number"  value="<%= campground.price %>"  name="camp[price]">
                    </div>
                    <div class="form-group">
                        <input type="number"  value="<%= campground.member %>"  name="camp[member]">
                        </div>
    <div class="form-group">
    <button>Add</button>
    </div>
</form>

<%- include("../partials/footer") %>